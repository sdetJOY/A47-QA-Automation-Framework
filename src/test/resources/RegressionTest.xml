<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Regression" parallel="methods" thread-count="2">
    <parameter name="baseURL" value="https://qa.koel.app/"/>

    <test name="Regression Tests" preserve-order="false">
        <classes>
            <class name="LoginTests">
                <methods>
                    <include name="loginValidEmailValidPasswordTest"/>
                    <include name="loginValidEmailEmptyPasswordTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>